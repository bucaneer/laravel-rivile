{
    "name": "itcity/laravel-rivile",
    "description": "Rivile integration for Laravel",
    "keywords": ["laravel", "rivile", "soap"],
    "license": "proprietary",
    "authors": [
        {
            "name": "Justas LaviÅ¡ius",
            "email": "jl@itcity.lt"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "5.7.*"
